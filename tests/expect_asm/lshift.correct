.ORIG x3000
LD R5, BOTTOM_OF_STACK
LD R6, BOTTOM_OF_STACK
JSR main
HALT
BOTTOM_OF_STACK .FILL xF000
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; int main()
main
ADD R6, R6, #-1
PUSH R7
PUSH R5
.COPY R5, R6
ADD R6, R6, #-2
; end of prologue
.ZERO R0
ADD R0, R0, #1
STR R0, R5, #-1
LD R0, imm30
LDR R1, R5, #-1 ; load a
ADD R1, R1, R1
ADD R1, R1, R1
ADD R0, R0, R1
OUT
.ZERO R0
ADD R0, R0, #2
STR R0, R5, #-2
LD R0, imm30_2
LDR R1, R5, #-1 ; load a
LDR R2, R5, #-2 ; load b
.SETCC R2
BRnz main_lshift_e
main_lshift_s
ADD R1, R1, R1
ADD R2, R2, #-1
BRp main_lshift_s
main_lshift_e
ADD R0, R0, R1
OUT
; epilogue
main_ret
ADD R6, R6, #2
POP R5
POP R7
RET
imm30 .FILL 0x30
imm30_2 .FILL 0x30
.END
